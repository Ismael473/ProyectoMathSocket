<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="TOP_RIGHT" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.proyectomathsocket.ControladorVentanaPrincipal">
    <padding>
        <Insets bottom="00.0" left="00.0" right="0.0" top="00.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <Button text="Dado!" onAction="#onHelloButtonClick"/>

    <Label fx:id="botonDeSalida"/>
    <Button text="Salida!" onAction="#onExitButtonClick"/>
</VBox>
